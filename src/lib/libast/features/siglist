lib	strsignal string.h signal.h

tst	- run{
	sig=$(env kill -l) || exit
	for sig in $sig
	do	case $sig in
		*[!A-Za-z0-9_]*|*MIN|*MAX)
			;;
		*)	echo "#if defined(SIG$sig) && !defined(HAD_SIG$sig)"
			echo "0,\"$sig\",SIG$sig,"
			echo "#endif"
			;;
		esac
	done
}end fail{
	echo "$0: failed to get list of signals from external 'kill -l'" >&2
	exit 1
}end
