	done ${INSTALLROOT}/include/ast/ast_mode.h
	make ${INSTALLROOT}/include/ast/ast_ndbm.h
		make ast_ndbm.h
			make FEATURE/ndbm
				prev features/ndbm
				prev std/stdio.h
				exec - iffe ${IFFEFLAGS} -v -X ast -X std -c "${CC} ${mam_cc_FLAGS} ${CCFLAGS} ${LDFLAGS}" run features/ndbm
			done FEATURE/ndbm
			exec - ${STDCP} -f FEATURE/ndbm ast_ndbm.h
		done ast_ndbm.h
		exec - ${STDCP} -f ast_ndbm.h ${INSTALLROOT}/include/ast/ast_ndbm.h
	done ${INSTALLROOT}/include/ast/ast_ndbm.h
	make ${INSTALLROOT}/include/ast/ast_param.h
