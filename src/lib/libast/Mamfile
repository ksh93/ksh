	done ${INSTALLROOT}/include/ast/ast_fcntl.h
	make ${INSTALLROOT}/include/ast/ast_float.h
		make ast_float.h
			prev FEATURE/float
			prev ast_common.h
			exec - ${STDCP} -f FEATURE/float ast_float.h
		done ast_float.h
