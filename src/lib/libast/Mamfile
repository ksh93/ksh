			make FEATURE/sizeof
				prev features/sizeof
				exec - iffe ${IFFEFLAGS} -v -X ast -X std -c "${CC} ${mam_cc_FLAGS} ${CCFLAGS} ${LDFLAGS}" run features/sizeof
			done FEATURE/sizeof
			exec - ${STDCP} -f FEATURE/sizeof ast_sizeof.h
		done ast_sizeof.h
		exec - ${STDCP} -f ast_sizeof.h ${INSTALLROOT}/include/ast/ast_sizeof.h
	done ${INSTALLROOT}/include/ast/ast_sizeof.h
