	done ${INSTALLROOT}/include/ast/ast_float.h
	make ${INSTALLROOT}/include/ast/ast_fs.h
		prev ast_fs.h
		exec - ${STDCP} -f ast_fs.h ${INSTALLROOT}/include/ast/ast_fs.h
	done ${INSTALLROOT}/include/ast/ast_fs.h
	make ${INSTALLROOT}/include/ast/ast_lib.h
		prev ast_lib.h
